{
  "expo": {
    "name": "Mood Tracker",
    "slug": "mood-tracker",
    "description": "Application that captures the mood of the students",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-images/icon-3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/app-images/splash-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-images/adaptive-icon-3.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/app-images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3e8126c0-1c37-49c1-b4aa-65eefd24ac82"
      }
    },
    "owner": "tsaperlein"
  }
}
