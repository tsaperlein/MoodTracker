{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-images/icon-2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/app-images/splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-images/adaptive-icon-2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/app-images/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
