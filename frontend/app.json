{
  "expo": {
    "name": "Mood Tracker",
    "slug": "mood-tracker",
    "description": "Application that captures the mood of the students",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-images/icon-3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/app-images/splash-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tsaperlein.mood-tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-images/adaptive-icon-3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tsaperlein.mood_tracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/app-images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d42faef6-71da-44a5-9f2d-85981a66f0ff"
      }
    },
    "owner": "tsaperlein",
    "updates": {
      "url": "https://u.expo.dev/d42faef6-71da-44a5-9f2d-85981a66f0ff"
    }
  }
}
